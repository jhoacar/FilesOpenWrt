
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option disabled '0'
	option channel '1'
	option country 'US'

config wifi-iface
	option network 'wwan'
	option ssid 'Breaking Rules 2.4GHz'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option bssid '98:77:E7:37:D3:68'
	option key 'Jhoancarrero11'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'Menu'
	option network 'lan'
	option encryption 'psk2'
	option key 'wififood'

